#!/usr/bin/env perl

use strict;
use warnings;
use lib qw(lib t/lib);

use DU::Schema;
use SQL::Translator;

my $schema = DU::Schema->connect({ dsn => 'dbi:SQLite::memory:' });
print scalar ($schema->storage->deployment_statements(
  $schema,
  'SQLite',
  undef,
  undef,
  { producer_args => { no_transaction => 1 } }
));

